{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "bavix/laravel-wallet": "2.3.0",
        "biscolab/laravel-recaptcha": "^4.0",
        "cinetpay/cinetpay-php": "^1.9",
        "div-art/shortlink": "^0.2.4",
        "doctrine/dbal": "2.5.13",
        "eduardostuart/face": "^2.0",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "hardevine/shoppingcart": "^3.0",
        "illuminate/mail": "^5.5",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.5",
        "jgrossi/corcel": "2.5.0",
        "jorenvanhocht/laravel-share": "^2.0",
        "ksubileau/color-thief-php": "^1.4",
        "laravel/framework": "5.5.*",
        "laravel/passport": "~4.0",
        "laravel/socialite": "^3.1",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "mcamara/laravel-localization": "^1.6",
        "nikkanetiya/laravel-color-palette": "^1.3",
        "paragonie/random_compat": "2.*",
        "paypal/paypal-checkout-sdk": "1.0.0",
        "paypal/rest-api-sdk-php": "^1.14",
        "realrashid/sweet-alert": "3.2.2",
        "simplesoftwareio/simple-qrcode": "~2",
        "softon/laravel-face-detect": "dev-master",
        "srmklive/paypal": "~1.0",
        "stefangabos/zebra_pagination": "^2.3",
        "stripe/stripe-php": "^6.8",
        "unicodeveloper/laravel-paystack": "^1.0",
        "zgabievi/promocodes": "2.2.1",
        "zizaco/entrust": "^1.9"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files" : [
            "app/Helper/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
